#include <stdio.h>
#define SIZE 20
int main(){
    int array[SIZE];
    int i;
    for (i = 0; i < SIZE; i++){
        printf("element - %d : ", i);
        scanf("%d", &array[i]);
    }
    printf("Elements in array are: ");
    for (i = 0; i < SIZE; i++){
        printf("%d ", array[i]);
    }
    printf("\n");
    return 0;
}
