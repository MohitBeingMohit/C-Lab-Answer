#include <stdio.h>

int main() {
    // Define a constant for the Celsius value
    const float celsius = 37.0; // You can change this value as needed

    // Calculate the Fahrenheit value
    float fahrenheit = (celsius * 9.0 / 5.0) + 32.0;

    // Print the result
    printf("The temperature in Fahrenheit is: %.2f\n", fahrenheit);

    return 0;
}
